.page-header
  .row
    .col-lg-3
      %h1 Search
    = form_tag search_path, method: :get, class: 'search_form' do
      .col-lg-8
        = search_field_tag :q, params[:q], class: 'form-control cl-lg-8',
          placeholder: 'Search users or repos'
      .col-lg-1
        = button_tag 'Search', class: 'btn btn-sm btn-info search_button'

.row
  .col-lg-3
    aaa
  .col-lg-9
    - @users.each do |user|
      = user.login
